//Source: https://leetcode.com/problems/two-sum/

#include <stdio.h>
int main() {

    int nums[] = {2,7,11,15};
    int ans, i, j, target = 9;
    int len = sizeof nums / sizeof nums[0];

    for(i=0; i<len; i++){
        for(j=0; j<len; j++){
            ans = nums[i] + nums[j];
            if(ans == target){
                printf("%d, %d\n", i,j);
            }   
        }
    }

return 0;
}

